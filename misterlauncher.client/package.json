{
  "name": "misterlauncher.client",
  "version": "0.0.1",
  "config": {
    "theme": "default",
    "coreui_library_short_version": "5.2",
    "coreui_library_docs_url": "https://coreui.io/angular/docs/"
  },
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.4",
    "@angular/cdk": "^18.0.4",
    "@angular/common": "^18.0.4",
    "@angular/compiler": "^18.0.4",
    "@angular/core": "^18.0.4",
    "@angular/forms": "^18.0.4",
    "@angular/language-service": "^18.0.4",
    "@angular/platform-browser": "^18.0.4",
    "@angular/platform-browser-dynamic": "^18.0.4",
    "@angular/router": "^18.0.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@coreui/angular": "~5.2.2",
    "@coreui/angular-chartjs": "~5.2.2",
    "@coreui/chartjs": "~4.0.0",
    "@coreui/coreui": "~5.0.2",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-angular": "~5.2.2",
    "@coreui/utils": "^2.0.2",
    "@microsoft/signalr": "^8.0.7",
    "@types/md5": "^2.3.5",
    "chart.js": "^4.4.3",
    "jest-editor-support": "*",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "ngx-scrollbar": "^13.0.3",
    "run-script-os": "*",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.5",
    "@angular/cli": "^18.0.5",
    "@angular/compiler-cli": "^18.0.4",
    "@angular/localize": "^18.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.7",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": "^18.19.0 || ^20.9.0",
    "npm": ">= 9"
  }
}
